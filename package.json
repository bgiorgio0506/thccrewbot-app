{
  "name": "thecrewbot-app",
  "productName": "thecrewbot-app",
  "version": "1.0.2",
  "description": "My Electron application description",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Giorgio",
    "email": "bgiorgio0506@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/bgiorgio0506/thecrewbot-app.git",
    "owner": "bgiorgio0506",
    "name": "thecrewbot_app"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/icons/ico/ico1.ico",
        "executableName": "thecrewbot-app"
      },
      "electronWinstallerConfig": {
        "iconUrl": "file://C:/thecrewbot-app/src/assets/icons/ico/ico1.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "thecrewbot_app",
            "iconUrl": "file://C:/thecrewbot-app/src/assets/icons/ico/ico1.ico",
            "asar": {
              "unpackDir": "{src/}"
            }
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "bgiorgio0506",
              "name": "thecrewbot-app"
            },
            "authToken": "",
            "dreaft": true,
            "prerelease": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "icon": "file://C:/thecrewbot-app/src/assets/icons/ico/ico1.ico",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.52",
    "@electron-forge/maker-deb": "^6.0.0-beta.52",
    "@electron-forge/maker-rpm": "^6.0.0-beta.52",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "css-loader": "^4.2.1",
    "dotenv-webpack": "^2.0.0",
    "electron": "9.1.2",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@electron-forge/publisher-github": "^6.0.0-beta.52",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "dotenv": "^8.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.2",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "irc": "^0.5.2",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.9.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "request": "^2.88.2",
    "tmi.js": "^1.5.0",
    "twitch-toolkit": "0.0.11",
    "update-electron-app": "^1.5.0",
    "url": "^0.11.0"
  }
}
