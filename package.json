{
  "name": "thecrewbot-app",
  "productName": "thecrewbot-app",
  "version": "1.2.1",
  "description": "My Electron application description",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint \"src/**/*.js\" \"src/**/*.jsx\" --fix"
  },
  "keywords": [],
  "author": {
    "name": "Giorgio",
    "email": "bgiorgio0506@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/bgiorgio0506/thecrewbot-app.git",
    "owner": "bgiorgio0506",
    "name": "thecrewbot_app"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/icons/ico/ico1.ico",
        "executableName": "thecrewbot-app"
      },
      "electronWinstallerConfig": {
        "iconUrl": "file://C:/thecrewbot-app/src/assets/icons/ico/ico1.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "thecrewbot_app",
            "iconUrl": "file://C:/thecrewbot-app/src/assets/icons/ico/ico1.ico",
            "loadingGif": "src/assets/gif/thecrewbot.gif",
            "asar": {
              "unpackDir": "{src/}"
            }
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "bgiorgio0506",
              "name": "thecrewbot-app"
            },
            "authToken": "",
            "draft": true,
            "prerelease": true
          }
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "icon": "file://C:/thecrewbot-app/src/assets/icons/ico/ico1.ico",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.13.13",
    "@babel/runtime": "^7.14.0",
    "@babel/runtime-corejs3": "^7.12.5",
    "auto-launch": "^5.0.5",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "body-parser": "^1.19.0",
    "crypto": "^1.0.1",
    "electron-devtools-installer": "^3.1.1",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.0",
    "elgato-stream-deck": "^4.0.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "follow-redirects": "^1.14.8",
    "form-data": "^3.0.0",
    "javascript-obfuscator": "^2.10.7",
    "passport": "^0.4.1",
    "passport-oauth": "^1.0.0",
    "passport-twitch": "^1.0.3",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "raw-body": "^2.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "request": "^2.88.2",
    "reverse-tunnel-ssh": "^1.1.0",
    "shelljs": "^0.8.4",
    "tmi.js": "^1.7.1",
    "update-electron-app": "^2.0.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "^6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "css-loader": "^4.3.0",
    "dotenv-webpack": "^2.0.0",
    "electron": "^10.2.0",
    "electron-rebuild": "^2.3.4",
    "eslint": "^7.22.0",
    "eslint-plugin-promise": "^4.2.1",
    "html-webpack-plugin": "^4.5.0",
    "media-query-plugin": "^1.4.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-loader": "^1.0.2",
    "style-loader": "^1.2.1",
    "webpack-obfuscator": "^2.6.0"
  }
}
